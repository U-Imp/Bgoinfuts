<view class='stores-list'>
  <view class='stores-list-wrap'>
    <view class="stores-list-header">
      <view class="header-text">
        <view class="header-text-title">发现列表</view>
      </view>
    </view>

    <view class='stores-list-cards-wrap'>
      <block wx:for-items="{{ getData}}" wx:key="{{index}}">
        <view class='content'>
          <view class='adv'>
            <image src='{{item.explainImg}}' data-explainUrl='{{item.explainUrl}}' bindtap='gotoExplainUrl'></image>
          </view>
        </view>
      </block>
    </view>

  </view>
</view>